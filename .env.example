# BSC Testnet Configuration
# Copy this file to .env and fill in your values

# Your wallet private key (with tBNB)
PRIVATE_KEY=0x

# Alchemy BSC Testnet RPC
ALCHEMY_RPC_URL=https://bnb-testnet.g.alchemy.com/v2/YOUR_API_KEY

# Optional: BscScan API Key for contract verification
BSCSCAN_API_KEY=

# OpenRouter API Key for AI analysis
OPENROUTER_API_KEY=sk-or-v1-

# TheGraph Studio Deploy Key
SUBGRAPH_DEPLOY_KEY=

# Backend Configuration
PORT=3000
NODE_ENV=development

# Subgraph URL (after deployment)
SUBGRAPH_URL=https://api.studio.thegraph.com/query/YOUR_DEPLOYMENT/whal-e/v0.0.1

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3001

# Optional: Telegram Bot for notifications
TELEGRAM_BOT_TOKEN=

# Optional: Alchemy Webhook ID for real-time monitoring
ALCHEMY_WEBHOOK_ID=
